{
  "name": "{{ pkgname }}",
  "version": "0.0.0",
  "description": "{{ description }}",
  "author": "{{{ user author }}}",
  {{~#if keywords}}
  "keywords": [{{#each keywords~}}
    "{{{this}}}"{{#unless @last}}, {{/unless}}
  {{~/each}}],
  {{/if~}}
  "main": "{{#if coffee}}lib/{{/if}}{{underscored pkgname}}",
  "repository": {
    "type": "git",
    "url": "git://github.com/hurrymaplelad/{{ reposlug }}.git"
  },
  "homepage": "https://github.com/hurrymaplelad/{{ reposlug }}",
  "bugs": "https://github.com/hurrymaplelad/{{ reposlug }}/issues",
  "dependencies": {
  },
  "devDependencies": {
    "coffee-script": ">=1.7.x",
    "mocha": "~1.x.x"
  },
  "scripts": {
    {{~#if coffee}}
    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "compile": "coffee --compile --output lib/ src/",
    {{/if~}}
    "test": "mocha"
  }
}
